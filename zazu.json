{
  "name": "zazu-translation",
  "version": "1.1.1",
  "description": "Translation and optionally add to Anki",
  "icon": "./dictionary.png",
  "stylesheet": "zazu/zazu-dict.css",
  "blocks": {
    "input": [
      {
        "title": "translate to chinese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "cn",
        "script": "src/cn.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to japanese",
        "id": "Translation-japanese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "ja",
        "script": "src/ja.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to english",
        "id": "Translation-english",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "en",
        "script": "src/en.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to spanish",
        "id": "Translation-spanish",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "es",
        "script": "src/es.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to french",
        "id": "Translation-french",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "fr",
        "script": "src/fr.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to italian",
        "id": "Translation-italian",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "it",
        "script": "src/it.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to polish",
        "id": "Translation-polish",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "pl",
        "script": "src/pl.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to portuguese",
        "id": "Translation-portuguese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "pt",
        "script": "src/pt.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to russian",
        "id": "Translation-russian",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "ru",
        "script": "src/ru.js",
        "debounce": 100,
        "connections": ["copy"]
      }
    ],
    "output": [
      {
        "id": "copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
