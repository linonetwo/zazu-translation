{
  "name": "zazu-translation",
  "version": "1.1.1",
  "description": "Translation and optionally add to Anki",
  "icon": "./dictionary.png",
  "stylesheet": "zazu/zazu-dict.css",
  "blocks": {
    "input": [
      {
        "title": "translate to chinese",
        "id": "translation-chinese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "cn",
        "script": "src/cn.js",
        "debounce": 100,
        "connections": ["add-to-anki"]
      },
      {
        "title": "translate to traditional chinese",
        "id": "translation-traditional-chinese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "tw",
        "script": "src/tw.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to japanese",
        "id": "translation-japanese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "ja",
        "script": "src/ja.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to english",
        "id": "translation-english",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "en",
        "script": "src/en.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to spanish",
        "id": "translation-spanish",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "es",
        "script": "src/es.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to french",
        "id": "translation-french",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "fr",
        "script": "src/fr.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to italian",
        "id": "translation-italian",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "it",
        "script": "src/it.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to polish",
        "id": "translation-polish",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "pl",
        "script": "src/pl.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to portuguese",
        "id": "translation-portuguese",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "pt",
        "script": "src/pt.js",
        "debounce": 100,
        "connections": ["copy"]
      },
      {
        "title": "translate to russian",
        "id": "translation-russian",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "ru",
        "script": "src/ru.js",
        "debounce": 100,
        "connections": ["copy"]
      }
    ],
    "output": [
      {
        "id": "copy",
        "type": "UserScript",
        "script": "src/utils/getResult.js",
        "connections": ["copy-to-clipboard"]
      },
      {
        "id": "copy-to-clipboard",
        "type": "CopyToClipboard",
        "text": "{value}"
      },
      {
        "id": "add-to-anki",
        "type": "UserScript",
        "script": "src/utils/addToAnki.js"
      }
    ]
  }
}
