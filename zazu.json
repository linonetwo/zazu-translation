{
  "name": "zazu-translation",
  "version": "1.1.1",
  "description": "Translation and optionally add to Anki",
  "icon": "./dictionary.png",
  "stylesheet": "zazu/zazu-dict.css",
  "blocks": {
    "input": [
        {
        "title": "translate chinese",
          "id": "input",
          "type": "PrefixScript",
          "space": true,
          "args": "Required",
          "prefix": "ch",
          "script": "zazu/ch.js",
          "connections": [
            "copy"
          ]
        },
      {
        "title": "translate english",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "en",
        "script": "zazu/en.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "translate spanish",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "es",
        "script": "zazu/es.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "translate french",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "fr",
        "script": "zazu/fr.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "translate italian",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "it",
        "script": "zazu/it.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "translate polish",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "pl",
        "script": "zazu/pl.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "translate portuguese",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "pt",
        "script": "zazu/pt.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "translate russian",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "ru",
        "script": "zazu/ru.js",
        "connections": [
          "copy"
        ]
      },
      {
        "title": "search for synonyms",
        "id": "input",
        "type": "PrefixScript",
        "space": true,
        "args": "Required",
        "prefix": "syn",
        "script": "zazu/synonym.js",
        "connections": [
          "copy"
        ]
      }
    ],
    "output": [
      {
        "id": "copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
